[{"number": 375077, "data": "2,2480,8178,45153,3648841,7979090,101130029", "name": "Smallest k such that Product_{i=0..n} (k-i) divides C(2k,k).", "link": ["Thomas Bloom, <a href=\"https://www.erdosproblems.com/396\">Problem 396</a>"], "program": ["(PARI) for(n=1,20,for(k=n+1,100000,if(binomial(2*k,k)%prod(i=0,n,k-i)==0,print(n,\" \",k);break)))", "(Python)", "from math import prod, comb", "def A375077(n):", "    a, c, k = prod(n+1-i for i in range(n+1)), comb(n+1<<1,n+1), n+1", "    while c%a:", "        k += 1", "        a = a*k//(k-n-1)", "        c = c*((k<<1)-1<<1)//k", "    return k # _Chai Wah Wu_, Jul 30 2024"], "xref": ["Cf. A000984, A120626, A129489."], "keyword": "nonn,hard,more", "offset": "1,1", "author": "_Ralf Stephan_, Jul 29 2024", "ext": ["a(5) from _Chai Wah Wu_, Aug 01 2024", "a(6)-a(7) from _Max Alekseyev_, Feb 25 2025"], "references": 2, "revision": 19, "time": "2025-02-25T19:06:44-05:00", "created": "2024-07-30T15:37:32-04:00"}, {"number": 120626, "data": "1,2,5,14,14,46,46,382,446,1662,1662,3582,3582,4094,4094,130942,130942,458238,458238,1040382,1046526,2883582,3129342,16760830,16760830,56623102,66584318,367001598,367001598,670826494,670826494,8589672446,12348030974", "name": "Least number k such that the k-th Catalan number C(2k,k)/(k+1) is divisible by n!.", "link": ["Max Alekseyev, <a href=\"/A120626/b120626.txt\">Table of n, a(n) for n = 1..189</a>"], "mathematica": ["f[0] = 1, f[n_] := Block[{k = f[n - 1]}, While[ !IntegerQ[ Binomial[2k, k]/(n!(k + 1))], k++ ]; k]; Array[f, 20]"], "xref": ["Cf. A000108, A375077."], "keyword": "nonn", "offset": "1,2", "author": "_Robert G. Wilson v_, Jun 19 2006", "ext": ["a(16)-a(17) from _Ryan Propper_, Oct 13 2006", "Further terms from _Ryan Propper_, Dec 25 2007", "Further terms from _Max Alekseyev_, Feb 04 2008"], "references": 6, "revision": 10, "time": "2025-02-25T17:12:25-05:00", "created": "2006-09-29T03:00:00-04:00"}]